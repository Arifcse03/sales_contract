  <SQLQuery>
    <![CDATA[SELECT DISTINCT CHARACTER1 BUYER,CHARACTER15 BuyerId
  FROM QA_RESULTS QR
 WHERE QR.PLAN_ID = (SELECT PLAN_ID
                       FROM QA_PLANS QP
                      WHERE QP.NAME = 'BUYER WISE DETAILS OM')
ORDER BY CHARACTER1]]>
