  <SQLQuery>
    <![CDATA[SELECT STYLE,CLASSIFICATION,BPO,BPO_ID,ITEM_DESCRIPTION,STYLE_DESCRIPTION,QTY AS QUANTITY,ROUND(FOB,2) AS UNITPRICE,
ROUND((QTY*FOB),2) AS "TOTALVALUE",SHIPDATE,ENDDATE,BPO_ID LINE_ID,INCOTERMS,VERSION_NO from XX_Order_Info_V where BPO_ID IN (
SELECT V.BPO_ID FROM XX_Order_Info_V V,XX_COM_SALES_CONTRACT_L_T XXSCL WHERE V.BPO_ID=XXSCL.BPO_ID AND (V.STYLE<>XXSCL.STYLE_NO OR V.QTY<>XXSCL.LAST_QTY OR V.FOB<>XXSCL.LAST_PRICE OR V.BPO<>XXSCL.BPO_NO OR V.SHIPDATE<>XXSCL.SHIP_EX_FACTORY_DATE)) AND LC_UNIT=:P_ORG_ID AND SEASON=:P_SEASON AND BUYER_ID=:P_BUYER_ID
UNION ALL
SELECT STYLE,CLASSIFICATION,BPO,BPO_ID,ITEM_DESCRIPTION,STYLE_DESCRIPTION,QTY AS QUANTITY,ROUND(FOB,2) AS UNITPRICE,
ROUND((QTY*FOB),2) AS "TOTALVALUE",SHIPDATE,ENDDATE,BPO_ID LINE_ID,INCOTERMS,VERSION_NO from XX_Order_Info_V where BPO_ID NOT IN
(SELECT XXSCL.BPO_ID FROM XX_COM_SALES_CONTRACT_L_T XXSCL,XX_COM_SALES_CONTRACT_H_T XXSCH
WHERE XXSCL.IMP_SALE_HEADER_ID = XXSCH.IMP_SALE_HEADER_ID
AND XXSCH.BUYER_ID = :P_BUYER_ID
AND XXSCH.SEASON = :P_SEASON
AND XXSCH.ORG_ID = :P_ORG_ID) AND LC_UNIT=:P_ORG_ID AND SEASON=:P_SEASON AND BUYER_ID=:P_BUYER_ID]]>
  </SQLQuery>
